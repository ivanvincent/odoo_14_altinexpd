<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.module.category" id="module_master_design">
            <field name="name">Master Design</field>
            <field name="sequence">1</field>
        </record>

        <record model="res.groups" id="group_master_design_user">
            <field name="name">User</field>
            <field name="category_id" ref="module_master_design"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record model="res.groups" id="group_master_design_manager">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_master_design"/>
            <field name="implied_ids" eval="[(4, ref('inherit_product.group_master_design_user'))]"/>
        </record>
    </data>
</odoo>