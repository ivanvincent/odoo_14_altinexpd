        </record>
        <record id="hr_rule_ded_bpjs_jaminan_pensiun" model="hr.salary.rule">
            <field name="name">BPJS Jaminan Pensiun</field>
            <field name="sequence" eval="106"/>
            <field name="code">BPJSJP</field>
            <field name="category_id" ref="hr_payroll.DED"/>
            <field name="condition_select">python</field>
            <field name="condition_python">result = contract.bpjs_ketenagakerjaan > 0</field>
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = -(contract.type_id.pensiun_karyawan*0.01)*contract.bpjs_ketenagakerjaan if contract.type_id.jht_nominal_max > (contract.type_id.pensiun_karyawan*0.01)*contract.bpjs_ketenagakerjaan else -contract.type_id.jht_nominal_max</field>
        </record>
        <record id="hr_rule_ded_bpjs_kesehatan" model="hr.salary.rule">
            <field name="name">BPJS Kesehatan</field>
            <field name="sequence" eval="107"/>
            <field name="code">BPJSKES</field>
            <field name="category_id" ref="hr_payroll.DED"/>
            <field name="condition_select">python</field>
            <field name="condition_python">result = contract.bpjs_kesehatan > 0</field>
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = -(contract.type_id.bpjs_kes_karyawan*0.01)*contract.bpjs_kesehatan if contract.type_id.bpjs_kes_nominal_max > (contract.type_id.bpjs_kes_karyawan*0.01)*contract.bpjs_kesehatan else -contract.type_id.bpjs_kes_nominal_max</field>
        </record>
