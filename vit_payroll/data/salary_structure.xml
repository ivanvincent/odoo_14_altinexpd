        <record id="hr_rule_overtime" model="hr.salary.rule">
            <field name="name">Overtime</field>
            <field name="sequence" eval="10"/>
            <field name="code">OVT</field>
            <field name="category_id" ref="hr_payroll.ALW"/>
            <field name="condition_select">none</field>
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = (worked_days.Overtime.number_of_hours/173) * contract.umk</field>
        </record>
        <record id="hr_rule_overtime_day_off" model="hr.salary.rule">
            <field name="name">Overtime Day Off</field>
            <field name="sequence" eval="11"/>
            <field name="code">OVT2</field>
            <field name="category_id" ref="hr_payroll.ALW"/>
            <field name="condition_select">python</field>
            <field name="condition_python">result = worked_days.Overtime2.number_of_days > 0</field>
            <field name="amount_select">code</field>
            <field name="amount_python_compute">((worked_days.Overtime2.number_of_hours/173) * contract.umk) + (0.2 *(worked_days.Overtime2.number_of_hours/173) * contract.umk)</field>
        </record>
        <record id="hr_rule_overtime_day_off_security" model="hr.salary.rule">
            <field name="name">Overtime Day Off Security</field>
            <field name="sequence" eval="12"/>
            <field name="code">OVT3</field>
            <field name="category_id" ref="hr_payroll.ALW"/>
            <field name="condition_select">python</field>
            <field name="condition_python">result = worked_days.Overtime2.number_of_days > 0</field>
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = worked_days.Overtime2.number_of_hours*18750</field>
        </record>
        <record id="hr_rule_overtime_meal" model="hr.salary.rule">
            <field name="name">Overtime Meal</field>
