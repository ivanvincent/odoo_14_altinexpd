            ref('hr_rules_thr'),
            ref('hr_rules_bonus'),
            ref('hr_rules_adjustment'),
            ref('hr_rules_total_allowance'),
            ref('hr_rule_ded_inhealth'),
            ref('hr_rules_potongan_lain'),            
