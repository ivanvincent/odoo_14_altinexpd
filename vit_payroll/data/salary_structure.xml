        <!--Deduction-->
        <record id="hr_rule_zakat_profesi" model="hr.salary.rule">
            <field name="name">Zakat Profesi</field>
            <field name="sequence" eval="101"/>
            <field name="code">ZP</field>
            <field name="category_id" ref="hr_payroll.DED"/>
            <field name="condition_select">python</field>
            <field name="condition_python">result = contract.potongan_zakat</field>
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = -0.025 *(contract.wage+contract.t_jabatan+contract.t_fungsional+contract.t_komunikasi+contract.t_transport+contract.t_makan+contract.t_kendaraan_dinas+inputs.TKMHL.amount)</field>
        </record>
        <record id="hr_rule_unpaid_leave" model="hr.salary.rule">
            <field name="name">Unpaid Leave</field>
            <field name="sequence" eval="102"/>
            <field name="code">UNPAID</field>
            <field name="category_id" ref="hr_payroll.DED"/>
            <field name="condition_select">python</field>
            <field name="condition_python">result = worked_days.Unpaid</field>
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = -(worked_days.Unpaid.number_of_days / 30 * (contract.wage + contract.t_makan))</field>
        </record>
