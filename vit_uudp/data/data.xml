<?xml version="1.0" encoding='UTF-8'?>
<odoo noupdate="1">

    <record id="alert_penyelesaian" forcecreate="True" model="ir.cron">
        <field name="name">Notifikasi ajuan yang belum penyelesaian</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="model_id" ref="vit_uudp.model_uudp"/>
        <field name="numbercall">-1</field>
        <field name="user_id" ref="base.user_root" />
        <field name="nextcall" eval="(DateTime.now() + timedelta(hours=24)).strftime('%Y-%m-%d 15:00:00')" />
        <field name="active" eval="True"/>
        <field name="doall" eval="False"/>
        <field name="code">model.alert_uudp_penyelesaian()</field>
    </record>




</odoo>