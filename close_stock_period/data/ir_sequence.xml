<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record forcecreate="True" model="ir.cron" id="auto_close_period_two_days_ago">
            <field name="name">Close Period Two Days Ago</field>
            <field name="active">True</field>
            <field name="model_id" ref="model_close_stock_period"/>
            <field name="state">code</field>
            <field name="code">model.auto_close_stock_period()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(hours=24)).strftime('%Y-%m-%d 17:30:00')"/>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>

    </data>
</odoo>