<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="laporan_papertube_form" model="ir.ui.view">
        <field name="name">laporan.papertube.form</field>
        <field name="model">laporan.papertube</field>
        <field name="arch" type="xml">
            <form string="">
                <header>
                </header>
                <sheet>
                    <group>
                        <field name="date_start"/>
                        <field name="date_end"/>
                    </group>
                    <footer>
                        <button name="action_generate_excel" type="object" string="Generate Excel" class="oe_highlight"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>

    <record id="laporan_papertube_action" model="ir.actions.act_window">
        <field name="name">Laporan Papertube</field>
        <field name="res_model">laporan.papertube</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
        <field name="view_id" ref="laporan_papertube_form"/>
    </record>

    <menuitem
        id="laporan_papertube_menu"
        name="Laporan Produksi Papertube"
        action="laporan_papertube_action"
        parent="mrp.menu_mrp_reporting"
        sequence="14"/>

</odoo>
