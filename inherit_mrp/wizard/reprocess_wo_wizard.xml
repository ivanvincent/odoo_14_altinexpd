<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="reprocess_wo_wizard_form" model="ir.ui.view">
            <field name="name">reprocess.wo.wizard.form</field>
            <field name="model">reprocess.wo.wizard</field>
            <field name="arch" type="xml">
                <form string="">
                    <header>
                    </header>
                    <sheet>
                        <group>
                            <field name="workorder_id"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="reprocess" type="object" string="Reprocess" class="oe_highlight"/>
                        <button name="cancel" type="special" string="Cancel" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="reprocess_wo_wizard_action" model="ir.actions.act_window">
            <field name="name">Reprocess</field>
            <field name="res_model">reprocess.wo.wizard</field>
            <field name="view_mode">form</field>
            <field name="view_id" eval="reprocess_wo_wizard_form"/>
            <field name="target">new</field>
        </record>

    
    
    
    </data>
    

</odoo>
