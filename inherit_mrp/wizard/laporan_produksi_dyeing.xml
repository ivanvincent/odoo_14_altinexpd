<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="laporan_produksi_dyeing_form" model="ir.ui.view">
            <field name="name">laporan.produksi.dyeing.form</field>
            <field name="model">laporan.produksi.dyeing</field>
            <field name="arch" type="xml">
                <form string="">
                    <header>
                    </header>
                    <sheet>
                        <group>
                            <field name="type"/>
                            <field name="mrp_type_id"/>
                            <field name="date_start"/>
                            <field name="date_end"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="action_generate" type="object" string="Generate" class="oe_highlight"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="laporan_produksi_dyeing_action" model="ir.actions.act_window">
            <field name="name">Laporan Produksi Dyeing</field>
            <field name="res_model">laporan.produksi.dyeing</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="view_id" ref="laporan_produksi_dyeing_form"/>
        </record>

        <menuitem
            id="laporan_produksi_dyeing_menu"
            name="Laporan Produksi Dyeing"
            action="laporan_produksi_dyeing_action"
            parent="mrp.menu_mrp_reporting"
            sequence="15"/>
    
    </data>
    

</odoo>
