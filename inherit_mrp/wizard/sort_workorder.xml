<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="sort_workorder_wizard" model="ir.ui.view">
            <field name="name">sort.workorder.wizard.from</field>
            <field name="model">sort.workorder.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="date_start"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="need_to_sort" string="Submit" type="object" class="oe_highlight" />
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>