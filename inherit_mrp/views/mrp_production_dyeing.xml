            <field name="res_model">labdip.extra</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{}</field>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('inherit_mrp.labdip_extra_production_tree')})]"/>
        </record>


        <record id="mrp_production_dyeing_action" model="ir.actions.act_window">
            <field name="name">Manufacturing Orders Dyeing</field>
            <field name="res_model">mrp.production</field>
            <field name="view_mode">tree,kanban,form,calendar,pivot,graph</field>
            <field name="context">{'default_type_id': 2,'production_dyeing': 1}</field>
            <field name="domain">[('type_id', '=', 2)]</field>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('inherit_mrp.mrp_production_dyeing_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('inherit_mrp.mrp_production_dyeing_form')})]"/>
        </record>

        <record id="mrp_production_dyeing_gantt_action" model="ir.actions.act_window">
            <field name="name">Manufacturing Orders Dyeing</field>
            <field name="res_model">mrp.production</field>
            <field name="view_mode">gantt</field>
            <field name="context">{'default_type_id': 2,'display_product_and_color':1}</field>
            <field name="domain">[('type_id', '=', 2),('mesin_id','!=',False)]</field>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'gantt', 'view_id': ref('inherit_mrp.mrp_prodoction_view_gantt')})]"/>
        </record>

        <record id="mrp_workorder_dyeing_gantt_action" model="ir.actions.act_window">
            <field name="name">Work Orders Dyeing</field>
            <field name="res_model">mrp.workorder</field>
            <field name="view_mode">gantt</field>
            <field name="context">{'display_date_planned':1,'display_product_and_color':1}</field>
            <field name="domain">[('production_type_id', '=', 2),('workcenter_id.is_planning', '=', True),('mesin_id','!=',False),('is_planning','=',True)]</field>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'gantt', 'view_id': ref('inherit_mrp.mrp_workcenter_dyeing_scr_view_gantt')})]"/>
        </record>

