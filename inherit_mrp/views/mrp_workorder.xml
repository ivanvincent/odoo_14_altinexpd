<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="mrp_work_order_inherit_tree">
            <field name="name">mrp.workorder.tree</field>
            <field name="model">mrp.workorder</field>
            <field name="inherit_id" ref="mrp.mrp_production_workorder_tree_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='production_id']" position="after">
                    <field name="mesin_id"/>
                    <field name="employee_id"/>
                    <field name="shift"/>
                </xpath>
                <xpath expr="//field[@name='state']" position="before">
                    <field name="note" optional="show"/>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="mrp_workorder_inherit_form">
            <field name="name">mrp.workorder.inherit.form</field>
            <field name="model">mrp.workorder</field>
            <field name="inherit_id" ref="mrp.mrp_production_workorder_form_view_inherit"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='time_tracking']" position="after">
                    <page string="Parameter">
                        <field name="parameter_ids">
                            <tree>
                                <field name="no_urut"/>
                                <field name="parameter_id"/>
                                <field name="plan"/>
                                <field name="actual"/>
                            </tree>
                        </field>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>