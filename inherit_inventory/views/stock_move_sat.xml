<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="stock_move_fat_form" model="ir.ui.view">
            <field name="name">stock.move.form</field>
            <field name="model">stock.move</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <field name="sat_line_ids" nolabel="1">
                            <tree editable="bottom">
                                <field name="defect_id"/>
                                <field name="quantity"/>
                            </tree>
                        </field>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="stock_move_fat_action" model="ir.actions.act_window">
            <field name="name">SAT</field>
            <field name="res_model">stock.move</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="view_id" ref="stock_move_fat_form"/>
        </record>
    
    </data>
    

</odoo>
