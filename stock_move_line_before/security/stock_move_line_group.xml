<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.module.category" id="module_stock_move_line_before">
            <field name="name">Stock Move Line Before</field>
            <field name="sequence">1</field>
        </record>

        <record model="res.groups" id="group_stock_move_line_before_user">
            <field name="name">Stock Move Line Before User</field>
            <field name="category_id" ref="module_stock_move_line_before"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record model="res.groups" id="group_stock_move_line_before_manager">
            <field name="name">Stock Move Line Before Manager</field>
            <field name="category_id" ref="module_stock_move_line_before"/>
            <field name="implied_ids" eval="[(4, ref('group_stock_move_line_before_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    </data>
</odoo>