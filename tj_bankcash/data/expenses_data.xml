<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for account expenses -->
        <record id="seq_account_expenses" model="ir.sequence">
            <field name="name">account.expenses.seq1</field>
            <field name="code">account.expenses</field>
            <field name="prefix">EXPENSE/%(year)s/%(month)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>