<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="account_bank_statement_image_form" model="ir.ui.view">
            <field name="name">stock.move.images.form</field>
            <field name="model">account.bank.statement.line</field>
            <field name="arch" type="xml">
                <form string="">
                    <header>
                    </header>
                    <sheet>
                        <field name="image_ids">
                            <tree editable="bottom">
                                <field name="image_binary" widget="image" class="oe_avatar" readonly="0"/>
                                <field name="image_desc"/>
                            </tree>
                        </field>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="account_bank_statement_image_action" model="ir.actions.act_window">
            <field name="name">Images</field>
            <field name="res_model">account.bank.statement.line</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="view_id" ref="tj_bankcash.account_bank_statement_image_form"/>
        </record>
    
    </data>
    

</odoo>
