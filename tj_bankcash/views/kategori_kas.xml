<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="kategori_kas_form" model="ir.ui.view">
            <field name="name">kategori.kas.form</field>
            <field name="model">kategori.kas</field>
            <field name="arch" type="xml">
                <form string="">
                    <header>
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="type"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="kategori_kas_tree" model="ir.ui.view">
            <field name="name">kategori.kas.tree</field>
            <field name="model">kategori.kas</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="name"/>
                    <field name="type"/>
                </tree>
            </field>
        </record>

        <record id="kategori_kas_action" model="ir.actions.act_window">
            <field name="name">kategori.kas.action</field>
            <field name="res_model">kategori.kas</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem
            id="master_menu"
            name="Master"
            parent="tj_bankcash_top"
            sequence="105"/>

        <menuitem
            id="kategori_kas_menu"
            name="Kategori Kas"
            action="kategori_kas_action"
            parent="master_menu"
            sequence="1"/>
    
    </data>
    

</odoo>
