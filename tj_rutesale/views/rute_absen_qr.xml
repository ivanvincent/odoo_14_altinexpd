<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>


        <record id="view_rute_absen_qr_tree" model="ir.ui.view">
            <field name="name">rute_absen_qr.tree</field>
            <field name="model">absensi.qr</field>
            <field name="type">tree</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="absen qr">
                    <field name="name"/>
                    <field name="rute_id"/>
                    <field name="datetime"/>
                    <field name="key_qr" />
                    <field name="time_start" />
                    <field name="time_expired" />
                    <field name="status_qr" />
                </tree>
            </field>
        </record>


        <record id="view_rute_absen_qr_form" model="ir.ui.view">
            <field name="name">rute_absen_qr.form</field>
            <field name="model">absensi.qr</field>
            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="absen qr">
                    <sheet>
                        <group col="4" string = "Group 1">
                            <field name="name"/>
                            <field name="rute_id"/>
                            <field name="datetime"/>
                            <field name="key_qr" />
                            <field name="time_start" />
                            <field name="time_expired" />
                            <field name="status_qr" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>