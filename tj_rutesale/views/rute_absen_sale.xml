<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>


        <record id="view_rute_absen_sale_tree" model="ir.ui.view">
            <field name="name">rute_absen_sale.tree</field>
            <field name="model">absensi.sale</field>
            <field name="type">tree</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="absen sale">
                    <field name="name"/>
                    <field name="date"/>
                    <field name="abs_qr_id"/>
                    <field name="sales_id" />
                    <field name="rute_id" />
                    <field name="datetime_out" />
                    <field name="datetime_in" />
                    <field name="session_key" />
                    <field name="imei_phone" />
                    <field name="uniq_phone_device" />
                    <field name="account_phone_profile" />
                    <field name="out_latitude" />
                    <field name="out_longitude" />
                    <field name="in_latitude" />
                    <field name="in_longitude" />
                    <field name="token_key" />
                    <field name="state" />
                </tree>
            </field>
        </record>


        <record id="view_rute_absen_sale_form" model="ir.ui.view">
            <field name="name">rute_absen_sale.form</field>
            <field name="model">absensi.sale</field>
            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="absen sale">
                    <sheet>

                        <group col="4" string = "Group 1">
                            <field name="name"/>
                            <field name="date"/>
                            <field name="abs_qr_id"/>
                            <field name="sales_id" />
                            <field name="rute_id" />
                            <field name="datetime_out" />
                            <field name="datetime_in" />
                            <field name="session_key" />
                            <field name="imei_phone" />
                            <field name="uniq_phone_device" />
                            <field name="account_phone_profile" />
                            <field name="out_latitude" />
                            <field name="out_longitude" />
                            <field name="in_latitude" />
                            <field name="in_longitude" />
                            <field name="token_key" />
                            <field name="state" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>