<odoo>
    <data>
        <record id="action_rute_sale_list" model="ir.actions.act_window">
            <field name="name">Rute Sale</field>
            <field name="res_model">rute.sale</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
            Click to add a rute sale
                </p>
            </field>
        </record>

        <record id="action_rute_group_list" model="ir.actions.act_window">
            <field name="name">Rute Group</field>
            <field name="res_model">rute.group</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
            Click to add a rute group
                </p>
            </field>
        </record>

        <record id="action_rute_partner_list" model="ir.actions.act_window">
            <field name="name">Route Partner</field>
            <field name="res_model">rute.partner</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
            Click to add a rute_sale
                </p>
            </field>
        </record>

        <record id="action_rute_absen_qr_list" model="ir.actions.act_window">
            <field name="name">Absen QR</field>
            <field name="res_model">absensi.qr</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
            Click to add a rute_sale
                </p>
            </field>
        </record>

        <record id="action_rute_absen_sale_list" model="ir.actions.act_window">
            <field name="name">Absen Sale</field>
            <field name="res_model">absensi.sale</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
            Click to add a Absen Sale
                </p>
            </field>
        </record>
        <record id="action_rute_new_cust_list" model="ir.actions.act_window">
            <field name="name">New Custumer</field>
            <field name="res_model">rute.sale.new.cust</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
            Click to add a New Cust
                </p>
            </field>
        </record>


        

        <menuitem id="rute_sale_menu" name="Rute Sale" sequence="20" web_icon="tj_rutesale,static/description/icon.png">
            <menuitem id="rute_sale_1" name="Rute Sale" sequence="1">
                <menuitem id="menu_rute_sale_konst" name="Rute Sale" action="action_rute_sale_list" sequence="1"/>
                <menuitem id="menu_rute_sale_new_cust" name="Rute New Cust" action="action_rute_new_cust_list" sequence="2"/>
            </menuitem>
            <menuitem id="rute_absen_menu" name="Absen" sequence="2">
                <menuitem id="menu_rute_absen_qr" name="Absensi QR" action="action_rute_absen_qr_list" sequence="1"/>
                <menuitem id="menu_rute_absen_sale" name="Absensi Sale" action="action_rute_absen_sale_list" sequence="2"/>
            </menuitem>
            <menuitem id="rute_sale_config_menu" name="Configuration" sequence="3">
                <menuitem id="menu_jalur" name="Jalur" action="dh_res_partner.action_partner_jalur" sequence="2"/>
                <menuitem id="menu_jalur_jwk" name="JWK" action="dh_res_partner.action_partner_jalur_line" sequence="3"/>
                <!-- <menuitem id="menu_rute_group" name="Master Rute Group" action="action_rute_group_list" sequence="4"/>
                <menuitem id="menu_rute_partner" name="Master Rute Customer" action="action_rute_partner_list" sequence="5"/> -->
            </menuitem>
        </menuitem>


