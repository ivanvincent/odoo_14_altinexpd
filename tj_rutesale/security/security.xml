<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.module.category" id="module_rute_sale">
            <field name="name">Rute Sale</field>
            <field name="sequence">1</field>
        </record>

        <record model="res.groups" id="group_rute_sale_user">
            <field name="name">User</field>
            <field name="category_id" ref="module_rute_sale"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record model="res.groups" id="group_rute_sale_manager">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_rute_sale"/>
            <field name="implied_ids" eval="[(4, ref('tj_rutesale.group_rute_sale_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    </data>
</odoo>