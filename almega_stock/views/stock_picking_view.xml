<odoo>
	<data>
		
		<record id="almega_stock_picking_form_view" model="ir.ui.view">
		    <field name="name">almega.stock.picking.form</field>
		    <field name="model">stock.picking</field>
		    <field name="type">form</field>
		    <field name="inherit_id" ref="stock.view_picking_form"/>
		    <field name="arch" type="xml">

		    	<button name="action_cancel" position="after">
		    		<button name="action_generate_serial" string="Generate Barcode" type="object" attrs="{'invisible': ['|',('picking_type_code','!=','incoming'),('state','!=','assigned')]}"/>
		    	</button>
		    	<field name="origin" position="after">
		    		
		    	</field>

		     </field>
		</record>

		
		<record model="ir.ui.view" id="stock_picking_inherit_tree">
			<field name="name">stock.picking.tree</field>
			<field name="model">stock.picking</field>
			<field name="inherit_id" ref="stock.vpicktree"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='partner_id']" position="after">
					<field name="list_product"/>
				</xpath>
			</field>
		</record>
        
	</data>
</odoo>