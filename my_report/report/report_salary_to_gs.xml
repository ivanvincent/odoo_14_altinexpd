<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="report.paperformat" id="paperformat_report_salary_gs">
            <field name="name">Report Salary GS A4</field>
            <field name="default" eval="True" />
            <field name="format">A4</field>
            <!-- <field name="page_width">70</field>
            <field name="page_height">100</field> -->
            <field name="orientation">Portrait</field>
            <field name="margin_top">5</field>
            <field name="margin_bottom">5</field>
            <field name="margin_right">5</field>
            <field name="margin_left">5</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">35</field>
        </record>

        <report 
            id="action_report_salary_gs" 
            model="hr.payslip"
            string="Print Salary to GS" 
            report_type="qweb-pdf" 
            name="my_report_report_salary_gs" 
            file="my_report_report_salary_gs" />

        <record id="action_report_salary_gs" model="ir.actions.report">
            <field name="paperformat_id" eval="paperformat_report_salary_gs" />
        </record>

        <template id="report_salary_gs">
        <h3>test</h3>
        </template>
    </data>
</odoo>