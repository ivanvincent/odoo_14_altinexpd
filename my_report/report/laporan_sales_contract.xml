<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>

        <record model="report.paperformat" id="laporan_sales_contract" >
            <field name="name">Sales Contract</field>
            <field name="default" eval="False" />
            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">45</field>
            <field name="margin_bottom">10</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">35</field>
        </record>
        
        <record id="action_laporan_modul_sales_contract" model="ir.actions.report">
            <field name="name">Sales Contract</field>
            <field name="model">sale.contract</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">my_report.laporan_modu_sales_contract</field>
            <field name="report_file">my_report.laporan_modu_sales_contract</field>
            <field name="print_report_name">'%s' % object.name</field>
            <field name="binding_model_id" ref="wibicon_quotation.model_quotation"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="sales_contract"/>
        </record>
