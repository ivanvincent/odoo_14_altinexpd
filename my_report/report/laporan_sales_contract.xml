<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>

        <record model="report.paperformat" id="sales_contract_modul" >
            <field name="name">Sales Contract</field>
            <field name="default" eval="False" />
            <field name="format">A4</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">10</field>
            <field name="margin_bottom">10</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">35</field>
        </record>
        
        <record id="action_report_print_out_sales_contract" model="ir.actions.report">
            <field name="name">Sales Contract</field>
            <field name="model">sale.contract</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">my_report.report_print_out_sales_contract</field>
            <field name="report_file">my_report.report_print_out_sales_contract</field>
            <field name="print_report_name">'%s' % object.name</field>
            <field name="binding_model_id" ref="my_report.model_sale_contract"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="followup_bb"/>
        </record>

        <template id="report_print_out_sales_contract">
        <t t-call="web.basic_layout">
          <td>test</td>
        </t>
        </template>
        
    </data>
</odoo>