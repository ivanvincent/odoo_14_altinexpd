<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">        <!-- Tips: File ini tidak di-baca ketika -u -->
        <record model="ir.sequence" id="mrp_request_sequence">
            <field name="name">MRP Request</field>
            <field name="code">mrp.request.sequence</field>
            <field name="prefix">MR/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
        </record>

        <record model="ir.sequence" id="work_order_daily_sequence">
            <field name="name">Work Order Daily</field>
            <field name="code">workorder.daily.code</field>
            <field name="prefix">WOD/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>