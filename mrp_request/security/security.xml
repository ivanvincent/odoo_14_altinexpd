<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.module.category" id="module_mrp_request">
            <field name="name">Mrp Request</field>
            <field name="sequence">1</field>
        </record>

        <record model="res.groups" id="group_mrp_request_user">
            <field name="name">User</field>
            <field name="category_id" ref="module_mrp_request"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record model="res.groups" id="group_mrp_request_manager">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_mrp_request"/>
            <field name="implied_ids" eval="[(4, ref('mrp_request.group_mrp_request_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    </data>
</odoo>