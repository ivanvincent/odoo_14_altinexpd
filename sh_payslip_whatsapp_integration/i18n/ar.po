#. module: sh_payslip_whatsapp_integration
#: model:mail.template,body_html:sh_payslip_whatsapp_integration.email_template_edi_payslip_custom
msgid ""
"\n"
"<div style=\"font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;margin: 0px; padding: 0px;\">\n"
"<p style=\"font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;margin: 0px; padding: 0px; font-size: 13px;\">\n"
"</p><ul style=\"margin:0px 0 1rem 0;list-style-type: none;\">\n"
"% if object.company_id.payroll_information_in_message and object.employee_id and object.company_id :\n"
"Hello *${object.employee_id.name},* %0A%0A\n"
"Your payslip for period *${object.date_from} to ${object.date_to}* is generated. %0A%0A\n"
"% endif \n"
"% if object.company_id.payroll_send_pdf_in_message :\n"
"\t\t\n"
"<li style=\"list-style-type: none;\">\n"
"*Click here to download Report* : ${object.payslip_url} %0A%0A%0A\n"
"</li>\t\n"
"% endif\n"
"Thank you.%0A%0A\n"
"\n"
"% if object.company_id.payroll_signature and object.env.user.sign :\n"
"            \t\n"
"<li style=\"list-style-type: none;\">%20 ${object.env.user.sign} %0A%0A</li>\n"
"% endif\n"
"</ul>\n"
"<p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\"></p>\n"
"</div>\n"
"\n"
"\t\t"
msgstr ""
"\n"
"<div style=\"font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;margin: 0px; padding: 0px;\">\n"
"<p style=\"font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;margin: 0px; padding: 0px; font-size: 13px;\">\n"
"</p><ul style=\"margin:0px 0 1rem 0;list-style-type: none;\">\n"
"% if object.company_id.payroll_information_in_message and object.employee_id and object.company_id :\n"
"مرحبا *${object.employee_id.name},* %0A%0A\n"
"payslip الخاص بك لفترة *${object.date_from} ل ${object.date_to}* تم إنشاؤه. %0A%0A\n"
"% endif \n"
"% if object.company_id.payroll_send_pdf_in_message :\n"
"\t\t\n"
"<li style=\"list-style-type: none;\">\n"
"*انقر هنا للتحميل* : ${object.payslip_url} %0A%0A%0A\n"
"</li>\t\n"
"% endif\n"
"شكرا لك.%0A%0A\n"
"\n"
"% if object.company_id.payroll_signature and object.env.user.sign :\n"
"            \t\n"
"<li style=\"list-style-type: none;\">%20 ${object.env.user.sign} %0A%0A</li>\n"
"% endif\n"
"</ul>\n"
"<p style=\"margin:0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\"></p>\n"
"</div>\n"
"\n"
"\t\t"

