<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        
        <record id="hitrory_receipts_view_tree" model="ir.ui.view">
            <field name="name">history.receipts.view.tree</field>
            <field name="model">history.receipts</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="picking_id"/>
                    <field name="min_date"/>
                    <field name="partner_id"/>
                    <field name="location_id"/>
                    <field name="location_dest_id"/>
                    <field name="picking_category"/>
                    <field name="product_id"/>
                    <field name="lot_id"/>
                    <field name="rak_id"/>
                    <field name="variasi_id"/>
                    <field name="product_uom_id"/>
                    <field name="qty" sum="qty"/>
                    <field name="pcs" string="Roll" sum="pcs"/>
                </tree>
            </field>
        </record>

        <record id="history_release_view_tree" model="ir.ui.view">
            <field name="name">history.release.view.tree</field>
            <field name="model">history.release</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="picking_id"/>
                    <field name="min_date"/>
                    <field name="partner_id"/>
                    <field name="location_id"/>
                    <field name="location_dest_id"/>
                    <field name="picking_category"/>
                    <field name="product_id"/>
                    <field name="lot_id"/>
                    <field name="rak_id"/>
                    <field name="variasi_id"/>
                    <field name="product_uom_id"/>
                    <field name="qty" sum="qty"/>
                    <field name="pcs" string="Roll" sum="pcs"/>
                </tree>
            </field>
        </record>
        
    </data>
</odoo>