<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">
        
        <record id="follow_up_forcast_wizard_view_form" model="ir.ui.view">
            <field name="name">follow.up.forcast.wizard.view.form</field>
            <field name="model">follow.up.forcast.wizard</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>
                            <field name="start_date"/>
                            <field name="end_date"/>
                        </group>
                        <footer>
                            <button name="action_generate" string="Open" type="object" class="oe_highlight"/>
                            <button string="Cancel" class="btn btn-default" special="cancel" />
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>
        
    </data>
</odoo>