<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>

        <record id="machine_form" model="ir.ui.view">
            <field name="name">machine.form</field>
            <field name="model">machine</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="machine_tree" model="ir.ui.view">
            <field name="name">machine.tree</field>
            <field name="model">machine</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="name"/>
                    <field name="code"/>
                </tree>
            </field>
        </record>
    
        <record id="machine_action" model="ir.actions.act_window">
            <field name="name">Machine</field>
            <field name="res_model">machine</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem
            id="machine_menu"
            name="Machine"
            action="machine_action"
            parent="menu_master"
            sequence="250"/>
    
    </data>
    

</odoo>
