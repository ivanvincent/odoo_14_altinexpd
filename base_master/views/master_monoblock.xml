<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <record id="tip_type_form" model="ir.ui.view">
            <field name="name">tip.type.form</field>
            <field name="model">tip.type</field>
            <field name="arch" type="xml">
                <form string="">
                    <header>
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="desc"/>
                                <field name="price"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="tip_type_tree" model="ir.ui.view">
            <field name="name">tip.type.tree</field>
            <field name="model">tip.type</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="name"/>
                    <field name="desc"/>
                    <field name="price"/>
                </tree>
            </field>
        </record>

       <record id="tip_type_action" model="ir.actions.act_window">
           <field name="name">Tip Type</field>
           <field name="res_model">tip.type</field>
           <field name="view_mode">tree,form</field>
       </record>

        <record id="basic_specification_mn_action" model="ir.actions.act_window">
            <field name="name">Basic Specification</field>
            <field name="res_model">basic.specification</field>
            <field name="context">{'default_type': 'MONOBLOCK'}</field>
            <field name="domain">[('type', '=', 'MONOBLOCK')]</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="material_mn_action" model="ir.actions.act_window">
