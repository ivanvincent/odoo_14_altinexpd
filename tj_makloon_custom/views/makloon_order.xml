                        <!--<field name="product_kerah"/>-->
                        <!--<field name="product_rib"/>-->
                        <!--<field name="product_manset"/>-->
                     </xpath>
                    <xpath expr="//notebook/page/field[@name='material_ids']/tree/field[@name='product_uom']" position="after">
                        <field name="product_kg"/>
                    </xpath>
                 </field>
            </record>



