                <search string="Search">
                	<field name="name"/>
                    <field name="order_id"/>
                    <field name="product_id"/>
                    <field name="product_lot"/>
                    <field name="product_warna_id"/>
                    <field name="state"/>
                </search>
            </field>
        </record>

