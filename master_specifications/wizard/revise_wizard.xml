<odoo>
    <data>

        <record id="revise_wizard_form" model="ir.ui.view">
            <field name="name">revise.wizard.form</field>
            <field name="model">revise.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <h3>Alasan revisi?</h3>
                        <field name="note" string="Note Revisi"/>
                    </sheet>
                    <footer>
                        <button string="Send" name="action_print" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="cancel_wizard_form" model="ir.ui.view">
            <field name="name">cancel.wizard.form</field>
            <field name="model">revise.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <h3>Alasan cancel?</h3>
                        <field name="note" string="Note Revisi"/>
                    </sheet>
                    <footer>
                        <button string="Send" name="action_submit_cancel" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


    </data>
</odoo>