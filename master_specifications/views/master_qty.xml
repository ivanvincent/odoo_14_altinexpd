<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="master_qty_tree" model="ir.ui.view">
            <field name="name">master.qty.tree</field>
            <field name="model">master.qty</field>
            <field name="arch" type="xml">
                <tree string="" editable="bottom">
                    <field name="name"/>
                    <field name="urutan"/>
                </tree>
            </field>
        </record>

        <record id="master_qty_action" model="ir.actions.act_window">
            <field name="name">Master Quantity</field>
            <field name="res_model">master.qty</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem
            id="master_qty_menu"
            name="Master Quantity"
            action="master_qty_action"
            parent="specifications_root_menu"
            sequence="21"
            groups="sales_team.group_sale_manager"/>
    
    </data>
    

</odoo>
