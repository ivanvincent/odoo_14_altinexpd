<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>

        <record id="qrf_seq" model="ir.sequence">
            <field name="name">QRF</field>
            <field name="code">quotation.request.form</field>
            <field name="prefix">Q-%(year)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="qrf_ic_no_doc" model="ir.sequence">
            <field name="name">QRF</field>
            <field name="code">qrf.ic.no.doc</field>
            <field name="prefix">IC/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

     
    </data>
    

</odoo>
