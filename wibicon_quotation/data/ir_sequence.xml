<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <record id="seq_quotation" model="ir.sequence">
            <field name="name">Quotation Seq</field>
            <field name="code">quotation</field>
            <field name="prefix">QO/%(y)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_engineering" model="ir.sequence">
            <field name="name">Request Engineering Seq</field>
            <field name="code">request.engineering</field>
            <field name="prefix">RE/%(y)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    
    </data>
    

</odoo>
