<odoo>
    <data>

        <record id="reporting_stock_xlsx_view" model="ir.ui.view">
            <field name="name">reporting.stock.xlsx.form</field>
            <field name="model">reporting.stock.xlsx</field>
            <field name="arch" type="xml">
                <form string="Reporting Stock">
                    <group>
                        <field name="reporting_id" readonly="1"/>
                        <field name="type" required="1"/>
                    </group>
                    <footer>
                        <button string="Export" name="action_export_xlsx" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


    </data>
</odoo>