<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="view_stock_return_picking_form">
            <field name="name">stock.return.picking.inherit.form</field>
            <field name="model">stock.return.picking</field>
            <field name="inherit_id" ref="stock.view_stock_return_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='location_id']" position="before">
                    <field name="production_id" options="{'no_open': True, 'no_create': True}" readonly="1" attrs="{'invisible':[('production_id', '=', False)]}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>