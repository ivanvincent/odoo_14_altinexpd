<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="mrp_production_type_weaving_form">
            <field name="name">mrp.production.form</field>
            <field name="model">mrp.production</field>
            <field name="inherit_id" ref="inherit_mrp.mrp_production_weaving_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='tgl_naik_beam']" position="before">
                    <field name="lot_id"/>
                    <field name="beam_id"/>
                    <field name="beam_type_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>