<odoo>
	<data>
		<record id="view_partner_kategori1_form" model="ir.ui.view">
			<field name="name">res.partner.kategori1.form</field>
			<field name="model">res.partner.kategori1</field>
			<field name="arch" type="xml">
				<form string="Partner Kategori 1">
					<sheet>
						<group>
							<group>
								<field name="name"/>
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>
		<record id="view_partner_kategori1_tree" model="ir.ui.view">
			<field name="name">res.partner.kategori1.tree</field>
			<field name="model">res.partner.kategori1</field>
			<field name="arch" type="xml">
				<tree string="Partner Kategori 1">
					<field name="name"/>
				</tree>
			</field>
		</record>
		<record id="view_partner_kategori1_search" model="ir.ui.view">
			<field name="name">res.partner.kategori1.search</field>
			<field name="model">res.partner.kategori1</field>
			<field name="arch" type="xml">
				<search string="Partner Kategori 1">
					<field name="name" filter_domain="[('name','ilike',self)]" string="Name"/>
				</search>
			</field>
		</record>
		<record id="action_partner_kategori1" model="ir.actions.act_window">
            <field name="name">Partner Kategori 1</field>
            <field name="res_model">res.partner.kategori1</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_partner_kategori1_search"/>
            <field name="view_id" ref="view_partner_kategori1_tree"/>
        </record>
        <menuitem action="action_partner_kategori1" id="menu_action_partner_kategori1" parent="sale.menu_sale_config" sequence="15"/>
	</data>
</odoo>