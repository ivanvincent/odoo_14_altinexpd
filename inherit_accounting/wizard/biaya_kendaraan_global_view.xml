<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="biaya_kendaraan_global_view_tree" model="ir.ui.view">
        <field name="name">biaya.kendaraan.global.view.tree</field>
        <field name="model">biaya.kendaraan.global.view</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="warehouse_id"/>
                <field name="model_id"/>
                <field name="license_plate"/>
                <field name="product_uom_qty" sum="1"/>
                <field name="price_unit" sum="1"/>
            </tree>
        </field>
    </record>

    <record id="biaya_kendaraan_global_view_action" model="ir.actions.act_window">
        <field name="name">Biaya Kendaraan Global View</field>
        <field name="res_model">biaya.kendaraan.global.view</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="biaya_kendaraan_global_view_tree"/>
    </record>

</odoo>
