<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="master_jenis_tree_new" model="ir.ui.view">
            <field name="name">master.jenis.new.tree</field>
            <field name="model">master.jenis.new</field>
            <field name="arch" type="xml">
                <tree string="" editable="bottom">
                    <field name="name"/>
                    <field name="type"/>
                    <field name="qty_ids" widget="many2many_tags"/>
                    <field name="sequence_id"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>

        <record id="master_jenis_action_new" model="ir.actions.act_window">
            <field name="name">Master Jenis New</field>
            <field name="res_model">master.jenis.new</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem
            id="specifications_root_new_menu"
            name="D-QUPS New"
            sequence="21"
            />

        <menuitem
            id="master_jenis_new_menu"
            name="Master Jenis"
            action="master_jenis_action_new"
            parent="specifications_root_new_menu"
            sequence="10"
            groups="sales_team.group_sale_manager"/>
    
    </data>
    

</odoo>
