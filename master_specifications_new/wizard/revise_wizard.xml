<odoo>
    <data>

        <record id="revise_wizard_form_new" model="ir.ui.view">
            <field name="name">revise.wizard.new.form</field>
            <field name="model">revise.wizard.new</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <h3>Silakan sebutkan alasan Anda melakukan revisi.</h3>
                        <p>*Alasan dapat dilihat di bagian pencatatan log di setiap halaman D-QUPS</p>
                        <field name="note" string="Note Revisi"/>
                    </sheet>
                    <footer>
                        <button string="Send" name="action_print" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="cancel_wizard_form_new" model="ir.ui.view">
            <field name="name">cancel.wizard.new.form</field>
            <field name="model">revise.wizard.new</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <h3>Silakan sebutkan alasan Anda melakukan cancel</h3>
                        <p>*Alasan dapat dilihat di bagian pencatatan log di setiap halaman D-QUPS</p>
                        <field name="note" string="Note Revisi"/>
                    </sheet>
                    <footer>
                        <button string="Send" name="action_submit_cancel" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


    </data>
</odoo>