<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_kontrabon_order" model="ir.sequence">
            <field name="name">kontrabon Order</field>
            <field name="code">kontrabon.order</field>
            <field name="prefix">KB-AP/%(y)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_kontrabon_piut" model="ir.sequence">
            <field name="name">Kontrabon Piut</field>
            <field name="code">kontrabon.order</field>
            <field name="prefix">KB-AR/%(y)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_kontrabon_new" model="ir.sequence">
            <field name="name">Kontrabon Piut</field>
            <field name="code">kontrabon.order.new</field>
            <field name="prefix">KB/PMTI/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_kontrabon_piutang_new" model="ir.sequence">
            <field name="name">Kontrabon Piutang</field>
            <field name="code">kontrabon.order.utang.new</field>
            <field name="prefix">KP/PMTI/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- <record id="seq_kontrabon_hut" model="ir.sequence">
            <field name="name">Kontrabon Hut</field>
            <field name="code">kontrabon.order</field>
            <field name="prefix">KB-H/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record> -->

    </data>
</odoo>
