<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="pull_mysql_wizard" model="ir.ui.view">
            <field name="name">pull.mysql.wizard</field>
            <field name="model">pull.mysql.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="start_date"/>
                            <field name="end_date"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="action_pull" string="Pull" type="object" class="oe_highlight" />
                        <button string="Cancel" class="oe_link" special="cancel" type="special"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>

    <record model="ir.actions.act_window" id="pull_mysql_action">
        <field name="name">Pull MySql</field>
        <field name="res_model">pull.mysql.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>


    <menuitem id="mysql_wizard_menu" name="Pull Invoice" parent="invoice_root_menu" action="pull_mysql_action" />



</odoo>