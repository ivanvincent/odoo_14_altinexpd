<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="print_inv_wizard" model="ir.ui.view">
            <field name="name">print.invoice.wizard</field>
            <field name="model">print.invoice.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="move_id" readonly="1" options="{'no_open': True, 'no_create': True}"/>
                            <field name="print_type" widget="radio"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="action_print" string="Print" type="object" class="oe_highlight" />
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>